  version: number

  tilesets: array
    name: string
    firstgid: number
    image: string
    imageheight: number
    imagewidth: number
    margin: number
    properties: object
    spacing: number
    tileheight: number
    tilewidth: number
    tileproperties: object

  layers: array
    data: array
    height: number
    name: string
    opacity: number
